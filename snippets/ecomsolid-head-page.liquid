
{% comment %}
  You SHOULD NOT modify source code in this page because
  It is automatically generated from EcomSolid
  Try to edit page with the live editor.
{% endcomment %}
{% if template.name == 'page' %}
  {% assign EcomSolid = page.metafields.EcomSolid %}
{% elsif template.name == 'product' %}
  {% assign EcomSolid = product.metafields.EcomSolid %}
{% elsif template.name == 'index' %}
  {% assign EcomSolid = shop.metafields.EcomSolid %}
{% elsif template.name == 'collection' %}
  {% assign EcomSolid = collection.metafields.EcomSolid %}
{% elsif template.name == 'article' %}
  {% assign EcomSolid = article.metafields.EcomSolid %}
{% elsif template.name == 'blog' %}
  {% assign EcomSolid = blog.metafields.EcomSolid %}
{% endif %}
{% if EcomSolid and EcomSolid['globalStyle'] %}
    {{ EcomSolid['globalStyle'] }}
{% endif %}
{% if EcomSolid and EcomSolid['headMeta'] %}
    {{ EcomSolid['headMeta'] }}
{% endif %}
{% if EcomSolid and EcomSolid['robots'] %}
    <meta name="robots" content="{{ EcomSolid['robots'] }}" />
{% endif %}
{% if EcomSolid and EcomSolid['facebookDesctiption'] %}
    <meta property="og:description" content="{{ EcomSolid['facebookDesctiption'] }}" />
{% endif %}
{% if EcomSolid and EcomSolid['title'] %}
    <meta property="og:title" content="{{ EcomSolid['title'] }}" />
{% endif %}
{% if EcomSolid and EcomSolid['thumbnail'] %}
    <meta property="og:image" content="{{ EcomSolid['thumbnail'] }}" />
    <meta property="og:image:alt" content="{{ page_title }}" />
{% endif %}
